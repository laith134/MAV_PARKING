<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mav_parking">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MAVPARKING">
        <activity android:name=".WarningViolation"></activity>
        <activity android:name=".ViewSchedule" />
        <activity android:name=".Validate" />
        <activity android:name=".UserForgotPass" />
        <activity android:name=".SubmitSchedule" />
        <activity android:name=".StudentParkingInformation" />
        <activity android:name=".ShuttleInfo" />
        <activity android:name=".RegisterUser" />
        <activity android:name=".RegisterGuest" />
        <activity android:name=".PaymentSuccess" />
        <activity android:name=".ParkingPermit" />
        <activity android:name=".ParkingInfo" />
        <activity android:name=".MakePayment" />
        <activity android:name=".Login" />
        <activity android:name=".HomeScreen" />
        <activity android:name=".GuestLogin" />
        <activity android:name=".EditSchedule" />
        <activity android:name=".Contact" />
        <activity android:name=".CashPayment" />
        <activity android:name=".CardPayment" />
        <activity android:name=".AddSchedule" />
        <activity android:name=".AddRemoveCar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>